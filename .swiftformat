--allman false
--indent 2
--indentcase true
--trimwhitespace always
--maxwidth 100
--wraparguments before-first
--wrapcollections before-first
--commas always
--semicolons never
--decimalgrouping 3
--binarygrouping 4
--hexgrouping 4
--octalgrouping 4
--exponentcase lowercase
--header ignore
--ifdef no-indent
--indentstrings false
--linebreaks lf
--patternlet hoist
--self remove
--stripunusedargs closure-only
--swiftversion 5.0 