{
  "name": "pinpad-client-maplibre",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:tiles": "serve -s ./assets/tiles -p 8080",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "test:maestro": "maestro test .maestro/",
    "test:maestro:ios": "bash tests/test-ios.sh",
    "test:maestro:android": "bash tests/test-android.sh",
    "generate-test-data": "cd assets && zip -r test.drift tiles",
    "prepare": "husky",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:kotlin": "bash -c \"[ -f .formatters/ktlint ] || (echo 'ktlint not found. Running setup...' && ./scripts/setup-formatters.sh) && .formatters/ktlint --format -F --relative android/**/*.kt\"",
    "format:swift": "bash -c \"[ -f .formatters/swiftformat ] || (echo 'swiftformat not found. Running setup...' && ./scripts/setup-formatters.sh) && .formatters/swiftformat ios/\"",
    "format:all": "yarn format && yarn format:kotlin && yarn format:swift",
    "setup:formatters": "./scripts/setup-formatters.sh"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5",
    "@maplibre/maplibre-react-native": "^10.0.0",
    "@react-navigation/native": "^7.0.0",
    "emoji-datasource": "^15.1.2",
    "expo": "~52.0.24",
    "expo-asset": "~11.0.1",
    "expo-blur": "~14.0.1",
    "expo-build-properties": "^0.13.2",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "^5.0.12",
    "expo-font": "~13.0.1",
    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.9",
    "expo-splash-screen": "~0.29.13",
    "expo-status-bar": "~2.0.0",
    "expo-system-ui": "~4.0.4",
    "expo-web-browser": "~14.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.3",
    "react-native-emoji-selector": "^0.2.0",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-web": "~0.19.13",
    "react-native-zip-archive": "^7.0.1",
    "serve": "^14.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "latest",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "hermes-engine": "^0.11.0",
    "husky": "^9.1.7",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.2",
    "lint-staged": "^15.4.3",
    "maestro-cli": "^1.1.10",
    "prettier": "^3.5.2",
    "react-test-renderer": "18.3.1",
    "string-width": "4.2.3",
    "typescript": "^5.3.3",
    "wrap-ansi": "7.0.0"
  },
  "private": true
}
