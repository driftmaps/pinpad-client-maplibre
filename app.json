{
  "expo": {
    "name": "pinpad-client-maplibre",
    "slug": "pinpad-client-maplibre",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.pinpadclientmaplibre",
      "infoPlist": {
        "CFBundleDocumentTypes": [
          {
            "CFBundleTypeName": "Drift File",
            "LSHandlerRank": "Owner",
            "CFBundleTypeExtensions": [
              "drift"
            ],
            "LSItemContentTypes": [
              "com.example.drift"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.pinpadclientmaplibre",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "mimeType": "*/*",
              "scheme": "file",
              "pathPattern": ".*\\.drift"
            },
            {
              "mimeType": "*/*",
              "scheme": "content",
              "pathPattern": ".*\\.drift"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "./withCustomInfoPlist",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "@maplibre/maplibre-react-native",
      "./plugins/withIosDeploymentTarget",
      [
        "expo-asset",
        {
          "assets": [
            "./assets/test.drift"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}