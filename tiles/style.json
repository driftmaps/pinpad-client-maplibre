{
  "version": 8,
  "sources": {
    "custom-tiles": {
      "type": "vector",
      "tiles": [
        "https://your-tile-server.com/tiles/{z}/{x}/{y}.pbf"
      ],
      "minzoom": 0,
      "maxzoom": 14
    }
  },
  "layers": [
    {
      "id": "custom-layer",
      "type": "fill",
      "source": "custom-tiles",
      "source-layer": "layer-name",
      "paint": {
        "fill-color": "#3388ff"
      }
    }
  ]
}
